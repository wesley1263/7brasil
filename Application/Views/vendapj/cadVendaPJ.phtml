				jQuery('#lb_valor_agente').hide();
				jQuery('#valor_agente').hide();
				jQuery('#valor_agente').val('');
				
				jQuery('.faturado').hide();
				jQuery('[name=faturado_vendaPJ]').val('');
			}
		});
		
		
		
